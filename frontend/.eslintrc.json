{
  "env": {
    "browser": true
  },
  "extends": ["airbnb", "plugin:vue/recommended", "prettier"],
  "globals": {
    "document": true,
    "swal": true,
    "window": true
  },
  "parserOptions": {
    "ecmaVersion": 8,
    "sourceType": "module"
  },
  "plugins": ["vue"],
  "rules": {
    "linebreak-style": "off",
    "no-console": "off"
  }
}
